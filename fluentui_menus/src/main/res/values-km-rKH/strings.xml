<resources>
    <!-- *** PopupMenu *** -->

    <!--accessibility-->
    <!--Describes the action used to toggle radio button and checkbox checked states.-->
    <string name="popup_menu_accessibility_item_toggle">បិទបើក</string>
    <!--Describes the action used to select a menu item.-->
    <string name="popup_menu_accessibility_item_select">ជ្រើសរើស</string>
    <!--Announces which menu item has been selected.-->
    <string name="popup_menu_accessibility_item_click_selected">%s ត្រូវបានជ្រើសរើស</string>
    <!--Announces which menu item has been checked.-->
    <string name="popup_menu_accessibility_item_click_checked">%s ត្រូវបានធីក</string>
