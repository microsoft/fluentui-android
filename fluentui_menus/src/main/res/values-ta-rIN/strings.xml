<resources>
    <!-- *** PopupMenu *** -->

    <!--accessibility-->
    <!--Describes the action used to toggle radio button and checkbox checked states.-->
    <string name="popup_menu_accessibility_item_toggle">நிலைமாற்றும்</string>
    <!--Describes the action used to select a menu item.-->
    <string name="popup_menu_accessibility_item_select">தேர்ந்தெடுக்கும்</string>
    <!--Announces which menu item has been selected.-->
    <string name="popup_menu_accessibility_item_click_selected">%s தேர்ந்தெடுக்கப்பட்டது</string>
    <!--Announces which menu item has been checked.-->
    <string name="popup_menu_accessibility_item_click_checked">%s தேர்வுசெய்யப்பட்டது</string>
    <!--Announces which menu item has been unchecked.-->
    <string name="popup_menu_accessibility_item_click_unchecked">%s தேர்வுநீக்கப்பட்டது</string>
    <!--Describes the radio button control.-->
    <string name="popup_menu_accessibility_item_radio_button">ரேடியோ பொத்தான்</string>
    <!--Describes the check box control.-->
    <string name="popup_menu_accessibility_item_check_box">தேர்வுப்பெட்டி</string>
    <!--Describes the state of a menu item that is checked.-->
    <string name="popup_menu_accessibility_item_state_checked">தேர்வுசெய்யப்பட்டது</string>
    <!--Describes the state of a menu item that is not checked.-->
    <string name="popup_menu_accessibility_item_state_not_checked">தேர்வுசெய்யப்படவில்லை</string>

</resources>